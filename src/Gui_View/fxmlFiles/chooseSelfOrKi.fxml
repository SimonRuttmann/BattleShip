<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" stylesheets="@../Stylesheets/DefaultTheme.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ChooseSelfOrKi">
   <children>
      <AnchorPane prefHeight="50.0" />
      <HBox alignment="CENTER" spacing="40.0" stylesheets="@../Stylesheets/DefaultTheme.css" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="playBySelf" mnemonicParsing="false" onAction="#playSelf" prefHeight="100.0" prefWidth="140.0" text="Selbst spielen" />
            <Button fx:id="letKiPlay" mnemonicParsing="false" onAction="#playKi" prefHeight="100.0" prefWidth="140.0" text="KI spielen lassen" />
         </children>
      </HBox>
      <AnchorPane prefHeight="50.0" />
   </children>
</VBox>
